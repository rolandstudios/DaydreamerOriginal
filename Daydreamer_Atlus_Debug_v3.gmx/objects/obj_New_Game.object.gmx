draw_set_font(fnt_BytePolice);
draw_set_halign(fa_left);
draw_set_valign(fa_center);

//FLASH INC ------
if (flash&gt;=1) flash_count*=-1;
if (flash&lt;=0) flash_count*=-1;
if (alarm[1]==-1)
flash+=flash_count;
else
flash+=8*flash_count;

//DRAW TEXT --------------------------------------------------------------
if(des_y==1*spacing+origin_y)
    { 
        draw_text_transformed_colour((menu_x+shift_front)*offset,(menu_y+2*spacing)*offset,"Game Start",offset,offset,0,image_blend,image_blend,image_blend,image_blend,flash); 
    }
    else 
    { 
        draw_text_transformed((menu_x)*offset,(menu_y+2*spacing)*offset,"Game Start",offset,offset,0);
    }
if(des_y==2*spacing+origin_y) 
    {
        draw_text_transformed_colour((menu_x+shift_front)*offset,(menu_y+3*spacing)*offset,"Stage Select",offset,offset,0,image_blend,image_blend,image_blend,image_blend,flash); 
    }
    else 
    { 
    draw_text_transformed((menu_x)*offset,(menu_y+3*spacing)*offset,"Stage Select",offset,offset,0);
    }
    
if(des_y==3*spacing+origin_y) 
    {
        draw_text_transformed_colour((menu_x+shift_front)*offset,(menu_y+4*spacing)*offset,"Back",offset,offset,0,image_blend,image_blend,image_blend,image_blend,flash);  
    }
    else  
    {
        draw_text_transformed((menu_x)*offset,(menu_y+4*spacing)*offset,"Back",offset,offset,0);
    }
    

    
if window_get_fullscreen()
{
image_xscale=.75;
image_yscale=.75;
}
else
{
image_xscale=.5625;
image_yscale=.5625;
}

image_alpha=1;
draw_sprite_ext(global.sprite[master.Menu_Arrow],image_index,x*offset,y*offset,image_xscale,image_yscale,0,c_white,1);

