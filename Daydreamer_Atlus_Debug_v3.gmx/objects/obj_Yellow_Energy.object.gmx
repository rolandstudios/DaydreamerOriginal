if (par_HERO.energy&gt;75) 
    {
        switch(par_HERO.weaponslot)
        {
        case 1: can_low_A=true; break;
        case 2: can_low_B=true; break;
        case 3: can_low_C=true; break;
        case 4: can_low_D=true; break;
        }
