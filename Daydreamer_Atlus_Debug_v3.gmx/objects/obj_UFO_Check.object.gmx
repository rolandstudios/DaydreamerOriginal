{
    if instance_exists(obj_UFO)
    { 
    nearest = instance_nearest( obj_camera_offset.x, obj_camera_offset.y,obj_UFO);
    
    targ_x=(((((nearest.x+last_x)*.5)+last_x)*.5+last_x)*.5+last_x)*.5;
    targ_y=(((((nearest.y+last_y)*.5)+last_y)*.5+last_y)*.5+last_y)*.5;
  
    point = point_direction( obj_camera_offset.x, obj_camera_offset.y,targ_x,targ_y);  
    dist = point_distance( obj_camera_offset.x, obj_camera_offset.y,targ_x,targ_y);
    
    if (dist&gt;250)  dist=250;
    if (dist&lt;50)  dist=50;
    
    arrow_phase+=.25*global.game_speed;
    dist+=10*sin(arrow_phase);
    
    x1 =  obj_camera_offset.x + lengthdir_x(dist, point); 
    y1 =  obj_camera_offset.y + lengthdir_y(dist, point); 
    
    arrow_index+=.5*global.game_speed;
    if (arrow_alpha&lt;1) arrow_alpha+=.05;
    draw_sprite_ext(spr_Pause_Arrow,arrow_index,x1,y1,1,1,point,c_white,arrow_alpha);
    
    last = nearest; 
    last_x=targ_x;
    last_y=targ_y;  
