        case 5: //Regular - High 
        if !audio_is_playing(snd_Text_A)
        audio_play_sound(snd_Text_B, 10, true);
        break;
        
        case 6: //Regular - Low
        if !audio_is_playing(snd_Text_B)
        audio_play_sound(snd_Text_B, 10, true);
        break; 
