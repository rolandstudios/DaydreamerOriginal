        if (par_HERO.slide) kick=5;
        if (par_HERO.jumpkick) kick=5;
        if (par_HERO.x&gt;x)
        ctl.kickback=-kick; else ctl.kickback=kick;
                
        ctl.hp=ctl.hp-obj_HERO_Melee.damage;
        
                
        if (alarm[3] &lt;=-1) 
        {   
        image_blend = c_red;
        alarm[3]=4;
        }
        
        }
    }
    else
    {

    
        //if (alarm[3]==-1) 
        ctl.hp=ctl.hp-shoot.damage;   
    
        if (alarm[3] &lt;=-1) 
        {   
        image_blend = c_red;
        alarm[3]=4; 
        }
