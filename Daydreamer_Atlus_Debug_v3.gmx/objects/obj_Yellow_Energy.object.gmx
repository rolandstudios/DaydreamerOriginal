    {
    var b=random_range(.5,1)*irandom_range(0,1);
    //var b=1.0;
    var c=0.0;
    shader_set(shd_bright_contrast);
    shader_set_uniform_f(global.brightness_amount , b);
    shader_set_uniform_f(global.contrast_amount, c)
    draw_sprite_ext(spr_Yellow_Energy,(par_HERO.energy*.25),x,y,offset,offset,image_angle,image_blend,image_alpha);
    shader_reset();
    }
    
    if (low)
    {
    if (irandom_range(1,3)==1)
    draw_sprite_ext(spr_Yellow_Energy,(par_HERO.energy*.25),x,y,offset,offset,image_angle,c_red,image_alpha);
    else
    draw_sprite_ext(spr_Yellow_Energy,(par_HERO.energy*.25),x,y,offset,offset,image_angle,c_white,image_alpha);
    }
}

