        <arguments>
          <argument>
            <kind>1</kind>
            <string>if window_get_fullscreen()
{
    offset=1.5;
}
else
{
    offset=1;
}

draw_set_color(c_black);
draw_set_alpha(.4);

//DRAW RECTANGLE
y_nudge=60;

if (grow_y&lt;190) grow_y+=20; //grow y wasd 140


draw_rectangle(0,(y_nudge+y+50-grow_y)*offset,(1280)*offset,(y_nudge+y+50+grow_y)*offset,false);
draw_set_color(c_white);

draw_line(0,(y_nudge+y+50-grow_y)*offset,(1280)*offset,(y_nudge+y+50-grow_y)*offset);
draw_line(0,(y_nudge+y+50+grow_y)*offset,(1280)*offset,(y_nudge+y+50+grow_y)*offset);


//DRAW CHECKERS
draw_set_color(c_black);
draw_set_alpha(.1);
size=30;

for (p=(y+50-grow_y);p&lt;(y+50+grow_y);p+=size) //TOP TO BOTTOM
    {
        if(check_shift==0) check_shift=size; else check_shift=0;
         
         
         for (n=-size;n&lt;1280;n+=size*2) //LEFT TO RIGHT
         {
             if (p&lt;=(y+50-grow_y))
             draw_rectangle((n+check_x+1*size)*offset,(y_nudge+y+50-grow_y)*offset,(n+check_x+size+1*size)*offset,(y_nudge+y+50-grow_y+check_y)*offset,false); //DRAW TOP SQUISH
            
             top=p+check_y;
             bottom=p+size+check_y;
             
             if bottom&gt;(y+50+grow_y) bottom=y+50+grow_y;
             
             if top&gt;(y+50+grow_y) top=y+50+grow_y;
             
            
             draw_rectangle((n+check_shift+check_x)*offset,(y_nudge+top)*offset,(n+size+check_shift+check_x)*offset,(y_nudge+bottom)*offset,false); //DRAW REGULAR TO BOTTOM
                
            
         }
    }
    
check_x+=.5;    
check_y+=.5;

if (check_x&gt;size) check_x=0;
if (check_y&gt;size) check_y=0;


if (sprite_exists(sprite_index))
{
//image_xscale=1.4;
//image_yscale=1.4;

//DRAW SHADOW
    if (shift==0)
    draw_sprite_ext(sprite_index,image_index,(x-1.5)*offset,(y+1.5)*offset,image_xscale*offset,image_yscale*offset,image_angle,c_black,.7);
    draw_sprite_ext(sprite_index,image_index,(x-shift)*offset,(y+shift)*offset,image_xscale*offset,image_yscale*offset,image_angle,c_black,.6);
    shader_set(shd_bright_contrast);
    shader_set_uniform_f(global.brightness_amount , brightness+global.b);
    shader_set_uniform_f(global.contrast_amount, contrast+global.c);
    draw_sprite_ext(sprite_index,image_index,(x)*offset,(y)*offset,image_xscale*offset,image_yscale*offset,image_angle,c_white,1);
    shader_reset();
}

if (awake_start)
{
//draw_sprite_ext(global.sprite[master.spr_Awakened],awake_index,(x+200-shift*.25)*offset,(y+60+shift*.25)*offset,image_xscale*offset,image_yscale*offset,image_angle,c_black,.5);
draw_sprite_ext(global.sprite[master.spr_Awakened],awake_index,(x+200)*offset,(y+60)*offset,image_xscale*.8*offset,image_yscale*.8*offset,image_angle,c_white,1);

if (awake_index&lt;=5)
{
    if !audio_is_playing(snd_flutter_tech)
    {
    audio_sound_pitch(snd_flutter_tech,1.1);
    audio_play_sound(snd_flutter_tech,8,false);
    }
    //if (alarm[2]==-1) alarm[2]=3;
}

if (awake_index&lt;=30) awake_speed=.5; //was 24
if (awake_index&gt;=sprite_get_number(global.sprite[master.spr_Awakened])-1) awake_speed=-.5;

    awake_index+=awake_speed;
}


