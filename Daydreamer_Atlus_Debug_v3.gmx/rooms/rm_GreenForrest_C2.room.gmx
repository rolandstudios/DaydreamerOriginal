  <code>global.World=7;
global.theme=10;
global.VParallax=true;

if(master.bg_GreenForrest==0)
{
master.b_num+=1;
master.bg_GreenForrest=master.b_num;
global.background[master.b_num]=background_add(working_directory+"\Imports\bg_GreenForrest.png",false,true);
}

background_index[0] = global.background[master.bg_GreenForrest];
background_index[1] = noone;
background_index[2] = noone;
background_index[3] = noone;
background_index[4] = noone;
background_index[5] = noone;
background_index[6] = noone;

background_xscale[0] = 1 * (room_width / background_width[0]);
background_yscale[0] = 1 * (room_height / background_height[0])

if audio_is_playing(amb_windstorm)
audio_stop_sound(amb_windstorm)

if audio_is_playing(amb_Psy)
audio_stop_sound(amb_Psy);

if audio_is_playing(amb_Factory)
audio_stop_sound(amb_Factory);

if audio_is_playing(amb_Caves)
audio_stop_sound(amb_Caves);

if !audio_is_playing(amb_birds)
audio_play_sound(amb_birds,10,true);

if !audio_is_playing(amb_Magical)
audio_play_sound(amb_Magical,10,true);

background_blend[0]= make_color_hsv(120,20,255); //GLOBAL BLEND

//Create HERO
if !instance_exists(par_HERO) 
{
switch(global.HERO_type)
{  
    case 0:
    with instance_create(400,1810,obj_HERO_young)
    {
    hp=global.SavedHP;
    money=global.SavedMoney;
    }   
    break;

    case 1:
    with instance_create(400,1810,obj_HERO)
    {
    hp=global.SavedHP;
    money=global.SavedMoney;
    }   
    break;
}
}</code>
