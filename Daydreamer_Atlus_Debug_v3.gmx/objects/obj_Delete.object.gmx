            <string>if (select_x==x+25) //YES
{
    if instance_exists(obj_SaveSelect)
    {
        if (global.platform==0) || (global.platform==1) //XBOX AND PC ONLY
        {
        var directory;
        
        switch(global.platform)
        {
        case 0: directory="SaveGame\"; break; //PC
        case 1: directory="SaveGame\"; break; //XBOXONE
        case 2: directory=""; break; //PS4
        }
                    
            switch(global.slot)
            {
            case 1: if file_exists(directory+"Save_A.sav"){file_delete(directory+"Save_A.sav");}  break;
            case 2: if file_exists(directory+"Save_B.sav"){file_delete(directory+"Save_B.sav");}  break;
            case 3: if file_exists(directory+"Save_C.sav"){file_delete(directory+"Save_C.sav");}  break;
            case 4: if file_exists(directory+"Save_D.sav"){file_delete(directory+"Save_D.sav");}  break;
            case 5: if file_exists(directory+"Save_E.sav"){file_delete(directory+"Save_E.sav");}  break;
            case 6: if file_exists(directory+"Save_F.sav"){file_delete(directory+"Save_F.sav");}  break;
            case 7: if file_exists(directory+"Save_G.sav"){file_delete(directory+"Save_G.sav");}  break;
            case 8: if file_exists(directory+"Save_H.sav"){file_delete(directory+"Save_H.sav");}  break;
            }
            
        }
    
    
        if (global.platform==3) //PS4 ONLY
        {
        //BLANK PLAYER DEFAULTS
        //------------------------
        global.date="- EMPTY -";  //DATE
        global.name="";             //HERO NAME
        global.SavedRoom  =  -1;    //CHECKPOINT ROOM ID
        global.SavedHP=100;         //Health           
        global.SavedMoney=0;        //Money
        
                                    
        for(var a=0; a&lt;=2; a+=1) //Set Inventory
        {
            for (b=0;b&lt;=3;b+=1)
            {
                global.Inventory[a,b]=0; //empty slot
            }
        } 
           
        global.weapon_A = 0; //WEAPON SLOT 1 ID
        global.weapon_B = 0; //WEAPON SLOT 2 ID
        global.weapon_C = 0; //WEAPON SLOT 3 ID
        global.weapon_D = 0; //WEAPON SLOT 4 ID
            
        global.pets[0]=1; //Turtle
        global.pets[1]=0; //Pup
        global.pets[2]=0; //Cat
        global.pets[3]=0; //Weasel
        global.pets[4]=0; //Duck
        
        global.weapon_A_energy=100; //WEAPON SLOT 1 AMMO
        global.weapon_B_energy=100; //WEAPON SLOT 2 AMMO
        global.weapon_C_energy=100; //WEAPON SLOT 3 AMMO
        global.weapon_D_energy=100; //WEAPON SLOT 4 AMMO
        
        global.Hours=0;    //Speed Run Time
        global.Minutes=0;  //Speed Run Time
        global.Seconds=0;  //Speed Run Time
        global.Points=0;   //SCORE
        global.HERO_type=1; //ADULT / CHILD HERO
        
        global.World=0;  //SET WORLD FOR WORLD MAP
        
        for (a=1;a&lt;=14;a+=1) //BOSS SCORE
        {  
            for (b=1;b&lt;=3;b+=1)
            {
                global.Boss_Score[a,b]=-1; //NOT BEATEN DEFAULT SCORE
            }
        }
        //WORLD PROGRESS
        for (c=1;c&lt;15;c+=1)
        {
            global.world_clear[c]=0; //WHICH WORLDS HAVE BEEN BEATEN
        }        
        global.ranked=false;          
        scr_savegame(); //NOW SAVE THE GAME
        } 
        
        
        obj_SaveSelect.player_name[global.slot] = "";
