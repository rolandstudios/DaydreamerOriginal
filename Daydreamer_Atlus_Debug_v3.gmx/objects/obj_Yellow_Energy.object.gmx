{
    switch(par_HERO.weaponslot)
    {
    case 1: if(can_low_A){if (par_HERO.energy&lt;25) {low=true;if(alarm[1]==-1){audio_play_sound(snd_alarm_Bleep,17,false); audio_play_sound(snd_low_ammo,18,false);alarm[1]=40;}}} break;
