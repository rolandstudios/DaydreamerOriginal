    draw_text_transformed((x+draw_x*.5+20)*offset,(y+35)*offset,"No",offset,offset,0);
    
    draw_sprite_ext(spr_arrow,0,(select_x)*offset,(y+35)*offset,1,1,0,c_white,1);
}
